@charset "UTF-8";
/**内容start**/
body {background:#f3f3f3;}
.content{
    width: 100%;
    padding:0;
    margin:0;
}
.content-box{
    width: 80%;
	min-width:1000px;
	background: #fff;
    margin: 0 auto;
    padding:30px 50px;
    margin-bottom:16px;
}
.information-nav{
    font-size: 14px;
    color: #999;
    line-height: 21px;
	margin-bottom:10px;
}
.information-nav a,.information-nav a:hover,.information-nav a:link,.information-nav a:focus{
	color:#999;
}
.information-nav a:hover{
	text-decoration: underline;
}
.content-box-l ,.content-box-r{
    width:435px;
    display: inline-block;
    vertical-align:top;
}
.content-box-l {
    padding-right: 30px;
}
.content-box-l .title,.content-box-r .title{
    height:50px;
    line-height: 50px;
    font-size: 20px;
    color: #333;
    border-bottom: 1px #eaeaea dashed;
    margin-bottom: 22px;
}
.content-box-l .title i,.content-box-r .title i{
    margin-right: 12px;
    color:#b19965;
}
.content-box-l .map,.content-box-r .map{
    width:100%;
    height: 252px;
}
.content-box-l .map a,.content-box-r .map a{
    display:block;
    width:100%;
    height:100%;
}
.content-box-l .info,.content-box-r .info{
    margin-top: 20px;
}
.content-box-l .info p,.content-box-r .info p{
    font-size: 14px;
    line-height: 32px;
    color: #333;
}
.content-b{
    width:100%;
}
.content-b-box{
    width: 80%;
	min-width:1000px;
    margin:0 auto;
	background: #fff;
	padding-bottom: 15px;
}
.content-b-l,.content-b-r{
    display: inline-block;
    font-size:14px;
    line-height: 21px;
}
.content-b-l{
    width:350px;
    padding-left:50px;
}
.content-b-r{
    width:645px;
}
.b-title{
    color:#c7b385;
    padding-top:16px;
}
.content-b-img{
    padding:15px 20px 0 0;
    width:75px;
    height:83px;
    float:left;
    display:inline;
}