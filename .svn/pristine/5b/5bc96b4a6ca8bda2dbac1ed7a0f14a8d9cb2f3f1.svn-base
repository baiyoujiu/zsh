@charset "UTF-8";
html, body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td {
    margin: 0;
    padding: 0;
    font-family: "Microsoft YaHei","arial","SimHei",sans-serif;
    font-size: 100%;
}
button,input,select,textarea {
    font: 12px/1.5 "Microsoft YaHei","arial","SimHei",sans-serif;
}
table {
    border-collapse: collapse;
    border-spacing: 0
}
fieldset,img {
    border: 0
}
address,caption,cite,code,dfn,em,strong,th,var {
    font-style: normal;
    font-weight: normal
}
li {
    list-style: none
}
a,a:hover,a:link,a:focus{
    text-decoration: none;
}
/**头部start**/
.o2owebsite-header{
    width:100%;
    background: #fff;
    border-bottom:1px #eaeaea solid;
}
.o2owebsite-header .o2owebsite-header-info{
    width:80%;
    min-width:1000px;
    height:60px;
    margin:0 auto;
    overflow: hidden;
}
.o2owebsite-leftblock{
    float: left;
}
.o2owebsite-rightblock{
    float: right;
    display: block;
}
.o2owebsite-logo-info{
    padding:9px 12px;
}
.o2owebsite-logo-info-top{
    color: #363741;
    font-size: 16px;
}
.o2owebsite-logo-info-bottom{
    color:#666;
    font-size: 12px;
    line-height:18px;
}
.o2owebsite-header-info-l a{
    cursor:default;
}
.o2owebsite-logo-img{
    display: inline-block;
}
ul.o2owebsite-header-nav{
    padding:0 0 0 30px;
}
ul.o2owebsite-header-nav li{
    display: block;
    float: left;
    padding:0 18px;
    height: 60px;
    line-height: 60px;
}
ul.o2owebsite-header-nav li a{
    color: #999;
    font-size: 18px;
}
ul.o2owebsite-header-nav li.o2owebsite-active a{
    color: #363741;
    font-weight: 700;
}
ul.o2owebsite-header-login{

}
ul.o2owebsite-header-login li{
    display: block;
    float: left;
    height:34px;
    line-height: 34px;
    margin:12px;
}
ul.o2owebsite-header-login li a{
    color: #999;
    font-size: 14px;
}
ul.o2owebsite-header-login li.o2owebsite-active{
    width:80px;
    text-align: center;
    -moz-border-radius: 50px;
    -webkit-border-radius: 50px;
    border-radius: 50px;
    border:1px solid #363741;
}
ul.o2owebsite-header-login li.o2owebsite-active a{
    color:#363741;
    display: block;
}
ul.o2owebsite-header-logined{

}
ul.o2owebsite-header-logined li{
    display: block;
    float: left;
    height: 20px;
    line-height: 20px;
    margin: 20px 0;
}
ul.o2owebsite-header-logined li a{
    color: #999;
    font-size: 14px;
    padding:0 16px;
    position: relative;
}
/*ul.header-logined li:last-child a{
    padding:0 0 0 16px;
}*/
ul.o2owebsite-header-logined li.o2owebsite-active a{
    color: #363741;
}
.o2owebsite-header-logined-line{
    position: absolute;
    height: 12px;
    width: 1px;
    background: #eaeaea;
    right: 0;
    top: 4px;
}
/**头部end**/

/**底部**/
.o2owebsite-footer{
    width: 100%;
    height: 140px;
    background: #363741;
}
.o2owebsite-footer .o2owebsite-footer-info{
    max-width: 720px;
    margin:0 auto;
    overflow: hidden;
}
.o2owebsite-footer-info-top{
    width:100%;
    padding-bottom: 20px;
    padding-top: 34px;
}
.o2owebsite-footer-info-top li{
    position: relative;
    display: block;
    float: left;
    width: 25%;
    text-align: center;
    height: 22px;
    line-height: 20px;
}
.o2owebsite-footer-info-line{
    position: absolute;
    height: 12px;
    width: 1px;
    background: #fff;
    right: 0;
    top: 6px;
}
.o2owebsite-footer-info-top li a{
    color: #fff;
    font-size: 14px;
}
.o2owebsite-footer-info p{
    clear: both;
    color: #fff;
    text-align: center;
    padding-bottom: 6px;
    font-size: 12px;
}
.o2owebsite-footer-info p a{
	color: #fff;
}
.o2owebsite-footer-info p i{
    padding-left:10px;
    font-style: normal;
}
/**底部end**/

/**备案号**/
.beian{
    margin-right: 4px;
    color:#fff;
}
.beian:hover,.beian:link,.beian:focus{
    color:#fff;
}
.beian img{
    width:16px;
    margin-top:-3px;
    vertical-align: middle;
    display:inline-block;
}

/**资讯中心css样式  后期删**/
.news .news-list li span{
    font-size:12px;
}
.boss-page{
    margin-bottom:40px !important;
    font-size:14px;
}