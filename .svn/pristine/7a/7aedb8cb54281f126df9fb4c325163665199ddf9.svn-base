.newstypelsit li img{width:190px;height:130px;position:absolute;left:0px;top:20px;border:1px solid #d8d8d8;}
.newstypelsit li .titlist{position:absolute;left:210px;top:10px;width:620px;}
.newstypelsit li .titlist span{font-size:16px;height:40px;color:#333;line-height:40px; font-weight:600;}
.newstypelsit li .titlist a span:hover{color:red;}
.newstypelsit li .titlist p{line-height:1.4;font-size:14px;color:#666;}
.newstypelsit li .keyword{position:absolute;left:210px;bottom:36px;width:620px;color:#999;}
.newstypelsit li .readlist{position:absolute;left:210px;bottom:10px;width:620px;}
.newstypelsit li .readlist em{font-style: normal;color:#666;}
.newstypelsit li .readlist span{float:right;color:#666;display:inline-block;margin-left:10px;cursor:pointer;}
.newstypelsit li .readlist span i{background:url(../images/news_icon.png) no-repeat;width:20px;height:19px;display:inline-block;margin-right:3px;}

.newstypelsit li .readlist span.readtime i{background-position: -24px -20px ;}
.newstypelsit li .readlist span.giveup i{background-position: -44px -20px ;}
.newstypelsit li .readlist span.forum i{background-position: -64px -21px ;}

.newstypelsit li .readlist span.readtime:hover i{background-position: -24px -1px ;}
.newstypelsit li .readlist span.giveup:hover i{background-position: -44px -1px ;}
.newstypelsit li .readlist span.forum:hover i{background-position: -64px -2px ;}


/*友情链接*/
.linkBox{width:1200px;margin:20px auto;*zoom:1;}
.linkBox .title{height:15px;text-align:center;font-size:18px;color:#333;border-bottom:#e2e2e2 solid 1px;}
.linkBox .title span{position:relative;top:5px;height:30px;padding:0 20px;display:inline-block;background:#f7f7f7;}
.linkBox .linkCon{overflow:hidden;padding:0 0 15px 0;line-height:30px;}
.linkBox .linkCon a{display:inline-block;margin-right:10px;}


/*当前位置样式*/
.currpoint{width:1200px;height:50px;line-height:50px;font-size:14px;margin:0px auto;}
.currpoint a{color:#999;}
.currpoint a.crrtab{color:#f55567;}

.bigtit{width:100%;font-size:18px;position:relative;height:30px;line-height:30px;border-bottom: 1px solid #e3e4e8;}
.bigtit .more{position:absolute;right:100px;color:#888;font-size:14px;}

/*阴影*/
.popup_bg{display:none; width:100%;height:100%; position:absolute;left:0;top:0;z-index:99998; background:#333; opacity:0.3;}
/*发布弹窗*/
.text_null{display:none;min-width:410px; background:#fff;min-height:170px; height:150px; border-radius:6px 6px; z-index:999999999999999; position:fixed; left:54%; top:40%; margin-left:-200px;}
.text_null_title{min-width:400px; line-height:40px; border-bottom:1px solid #F56276; text-align:center; font-size:16px; font-weight:600; color:#F56276;}
.text_null_content{text-align:center; line-height:70px; font-size:20px;padding: 5px 10px;}
.text_null_btn span{width:80px; height:30px; background:#009EE8; color:#fff; text-align:center; line-height:30px; border-radius:3px 3px; margin-right:15px; cursor:pointer;}
.text_null_content .gp_dui{background:url(/peasstatic/okpea2017/images/gp-dui.png) no-repeat; width:55px; height:38px; display: block; margin:17px 0 0 70px;}
.text_null_content .gp_cuo{background:url(/peasstatic/okpea2017/images/gp-cuo.png) no-repeat; width:55px; height:38px; display: block; margin:17px 0 0 8px;}