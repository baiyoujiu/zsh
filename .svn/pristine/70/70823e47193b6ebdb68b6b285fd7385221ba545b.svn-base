/* 经营数据 (cyy) 2016-05-17 */
.m-operation-atindex{}
.m-operation-atindex .media-info{padding:25px 20px 15px;border-left:1px solid #ddd;}
.m-operation-atindex .media-info:first-child{border:none;}
.m-operation-atindex .media-info .m-title{font-size:15px;}

.m-operation-dataone{position:relative;margin-bottom:20px;}
.m-operation-dataone .m-btn-return{position:absolute;right:0;top:0;}
.m-operation-dataone .title{background:transparent;}
.m-operation-dataone .content{padding:12px 0;}
.m-operation-dataone .pull-right{margin-bottom:0;}
.m-operation-date{padding:12px 0;}
.m-operation-date .m-date-select{display:inline-block;margin-left:30px;}
.m-operation-date .m-date-select .m-title{display:inline-block;font-weight:bold;line-height:30px;}
.m-operation-date .m-date-select .m-input{display:inline-block;width:150px;}
.m-operation-date .m-date-select .m-input input{padding-right:30px;}
.m-operation-info{position:relative;padding:35px 0;border:1px solid #ddd;}
.m-operation-info.m-tag-gk:before{content:"";position:absolute;left:12px;top:0;width:32px;height:53px;background:url(../img/tag_gaikuang_detail.png) no-repeat center top;}

.m-operation-info-block{float:left;position:relative;}
.m-operation-info-block:before{content:"";position:absolute;left:0;top:0;bottom:0;width:1px;background:#ddd;}
.m-operation-info-block:first-child:before{display:none;}
.m-operation-info-block .m-info{text-align:center;}
.m-operation-info-block .m-info.m-info-sub{margin-top:35px;}
.m-operation-info-block .m-info .m-number{font-size:26px;}
/*#excel-download{margin-right:6px;padding:3px 6px;border-radius:3px;color:#fff;background: #E52141\0;background: -webkit-linear-gradient(#EF2646 5%, #DC1534 40%);background: -o-linear-gradient(#EF2646 5%, #DC1534 40%);background: linear-gradient(#EF2646 5%, #DC1534 40%);border: 1px solid #BF041F;}
#excel-download:hover{background: #d2322d;border-color:#ac2925;}*/
#excel-download{margin-right:6px;padding:3px 6px;border-radius:3px;color:#fff;background:#e85352;border: 1px solid #e85352;}
#excel-download:hover{background: #fff;border-color:#e85352;color:#e85352;}
.operation-seach-cond{}
.operation-seach-cond .m-title{width:14%}
.operation-seach-cond .m-input{width:36%}
.operation-seach-handle{text-align:center;}
.operation-seach-handle .btn{width:80px;margin-top:20px;}
.m-input-tagline{position:relative;}
.m-input-tagline:before{content:"-";position:absolute;left:-4px;line-height:34px;color:#999;}
.m-client-detail p{margin:10px 0;color:#666;}
.m-client-remark{width:60%;border:1px dashed #999;padding:5px 10px;border-radius:5px;word-break:break-all;}
.m-client-remark.not-remark{color:#999;}
.m-remarkedit-tit{margin-bottom:10px;color:#666;}
.m-give-point{padding:20px 0;}
.m-give-point .form-group{width:250px;}
.m-give-point-handle{padding:20px 0;}

.nav-tabs .tips-right{float:right;cursor:pointer;margin-right:20px;margin-top:10px;}
.nav-tabs .tips-right i{font-size:16px;}
.m-msg-tips p{margin:0.5em 0;}

.m-line-title{margin:20px 10px 15px;padding-bottom:15px;border-bottom:1px dashed #b5b5b5;font-weight:bold;font-size:15px;}
.m-table-msg-list{width:100%;}
.m-table-msg-list td,.m-table-msg-list th{padding:6px 10px;}

.m-rowslist-module{font-size:12px;position:relative;}
.m-rowslist-title{border-top:1px solid #ddd;background:#f4f4f4;position:absolute;left:0;top:0;right:17px;}
.m-rowslist-body{padding-top:33px;max-height:330px;overflow-y:scroll;}
.m-rowslist-one{border:1px solid #ddd;border-top:none;}
.m-rowslist-one:after{content:".";display:block;height:0;visibility: hidden;clear: both;}
.m-rowslist-module .m-cell{float:left;padding:6px 2px;text-align:center;width:10%;line-height:20px;border-left:1px solid #ddd;box-sizing:border-box;-webkit-box-sizing:border-box;-moz-box-sizing:border-box;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.m-rowslist-module .m-cell:first-child{border-left:none;}

.m-dispatch-list .title{background:transparent;}


.default-setting{color:#428bca;cursor:pointer;}
.default-setting.setted{color:#333;cursor:default;}
.default-setting[disabled]{color:#999;cursor:not-allowed;}


/* 设置 样式 */
.m-setting-common{margin:15px 5px;line-height:34px;}
.m-setting-common .m-setting-title{float:left;margin-right:20px;margin-left:8px;line-height:36px;}
.m-setting-common .m-setting-value{overflow:hidden;}
.m-setting-common .m-onoff-new{position:relative;top:6px;}
.m-setting-common .m-icon-tip{margin-left:5px;color:#cdcdcd;font-size:14px;}

/***用户、店铺信息***/
.user_name{padding:12px 0 0 12px;height:87px;}
.user_name .img{float:left;display:inline;width:75px;height:75px;border:1px solid #dddddd;-moz-border-radius: 50%;-webkit-border-radius: 50%;border-radius:50%;background-size:75px;}
.user_name .img img {max-width: 100%;max-height: 100%;position: absolute;left: 0;right: 0;bottom: 0;top: 0;margin: auto;}
.user_name .message{padding:12.5px;line-height:1.7em;float:left;display:inline;}


.tip-extend-time p{margin:10px 0;}
.tip-extend-time select{color:#666;}

.box_cc_text{background:#fff;border-radius: 5px;margin-left: 10px;margin-bottom: 10px;zoom: 1;}
.box_cc_title{height:56px;padding:16px;border-bottom: 1px #dfdfdf solid;}
.box_cc_title_l,.box_cc_title_r{line-height:24px;}
.box_cc_title_l{font-size: 16px;}
.box_cc_title_l span{color:#333;/*font-weight:bold;*/}
.box_cc_title_r{text-align:right;}
.box_cc_title_r span{color:#999;font-size: 14px;margin-right:10px;}
.box_cc_main{padding:16px;}
.box_cc_main_text{color:#666;font-size: 14px;line-height: 21px;}

.arrow_defaul{display:none;}
.footer{background: none;}
.version{background: #484c4f;}

