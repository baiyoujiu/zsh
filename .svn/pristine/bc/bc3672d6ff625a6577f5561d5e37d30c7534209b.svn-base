body{background:#f5f5f5;}
.box{overflow:hidden;}
.zanwei{height:0.25rem;}

.head_top h2 a{text-align:center;line-height:2rem ;color:#f64353;font-size:0.8rem; font-weight:bold;}
.link_page_boxs {border-bottom: 1px solid #ccc;width:100%;position: fixed; background-color:#ffffff; z-index:66;}
.link_page_boxs ul {margin:auto; height:1.5rem;}
.link_page_boxs ul li { display: inline-block; position:relative; width:32%; height:1.5rem;line-height: 2rem;}
.link_page_boxs ul li a strong {font-size:1rem;font: inherit;}
.link_page_boxs ul li a.active {background-color: #ffffff;height:1.5rem; font-weight:bold; padding: 0 0.1rem;background:#f64353}
.link_page_boxs ul li strong.active {color:#fff;}
.link_page_boxs ul li a {display: block;position: absolute;text-align: center;width: 100%;height: 1.5rem;font-size: 0.8rem;}
/*banner*/
.banner{position:relative;}
.banner img{width:100%; height:8rem;}
.banner_son{position:absolute; left:0; top:0;}
.banner h1{font-size:0.8rem; color:#ffb600; line-height:2; padding-top:1rem; padding-left:0.25rem;}
.banner p{font-size:0.6rem; padding-left:0.25rem; line-height:1.5; color:#fff;height:4.5rem;overflow: hidden;}
.banner p i{color:#c00; font-size:0.7rem;}
