/*列表*/
.clearfix:before,.clearfix:after {content:"";display:table;}
.clearfix:after {clear:both;overflow:hidden;}
.clearfix {zoom:1; }
.fl {float:left;}
.fr {float:right;}
table{border-spacing:0;border-collapse:collapse; width:100%;}
td,th{padding:0; line-height:35px; font-size:13px;}
thead td,tbody td{text-align:center;}
.label{border:1px solid #000;}
.table{border-collapse:collapse!important}
.table td,.table th{background-color:#fff!important}
.table-bordered td,.table-bordered th{border:1px solid #ddd!important}

.main_right{min-width:1050px;}
.food_listss{margin:15px 0;}
.food_list_title tr{height:28px; line-height:28px; border:1px solid #e8e8e8; background:#f5f5f5;}
.food_list_title tr th{padding:5px 0;}
.food_list_title .food_name{width:400px; text-align:center;}
.food_list_title .food_price{width:107px; text-align:center;}
.food_list_title .food_number{width:54px; text-align:center;}
.food_list_title .food_handle{width:128px; text-align:center;}
.food_list_title .food_prices{width:128px; text-align:center;}
.food_list_title .food_state{width:87px; text-align:center;}
.food_list_title .food_transaction{width:128px; text-align:center;}

.food_list_lists{padding:10px 0;}
.food_list_lists .food_name{width:400px; padding:10px 0;}
.food_list_lists .food_price{width:107px; text-align:center;}
.food_list_lists .food_number{width:54px; text-align:center;}
.food_list_lists .food_handle{width:128px; text-align:center;}
.food_list_lists .food_prices{width:128px; text-align:center;}
.food_list_lists .food_state{width:87px; text-align:center;}
.food_list_lists .food_transaction{width:128px; text-align:center;}
.food_list_lists .food_name .grzx_food_img{width:80px; height:80px; margin:0 10px;}
.food_list_lists .food_name section{width:300px;}
.food_list_lists .food_name section p{width:300px; font-size:12px; color:#333; line-height:18px; height:36px; overflow:hidden; cursor:pointer;}
.food_list_lists .food_name section .food_name_fenlei{line-height:1; margin:8px 0; color:#999;}
.food_list_lists .food_name section p img{margin-right:10px; cursor:pointer;}

.mar_top10{margin-top:10px; border:1px solid #daf3ff; background:#fff;}
.mar_top10:hover{border:1px solid #aed8ff;}
.food_price_list .yj{text-decoration:line-through; font-size:12px; color:#999; padding:2px 0;}
.food_price_list .xj{font-size:12px; color:#333; padding:2px 0;}
.food_handle_list p{padding:2px 0; cursor:pointer;}
.food_prices_list p{padding:2px 0; cursor:pointer;}
.food_prices_list .xj{font-weight:600; font-size:13px;}
.food_prices_list .yf{color:#777;}
.food_state_list p{padding:2px 0; cursor:pointer;}
.food_transaction_list p{padding:2px 0; cursor:pointer;}

.food_list_head{height:35px; line-height:35px; background:#eaf8ff; border-bottom:1px solid #daf3ff;}
.food_list_head .food_dingdan{width:400px; padding-left:10px;}
.food_list_head .food_dingdan b{font-size:13px; margin-right:15px;}
.food_list_head .food_shop_name{width:250px; text-align: center; cursor:pointer;}
.food_list_head .food_shop_name:hover{color:#c00;}
.food_list_head .food_delete{width:80px; text-align:center; cursor:pointer; color:#888;}
.food_list_head .food_delete:hover{color:#c00;}
.food_list_head .food_shop_none{width:295px;}

.personal_coupon{padding:5px 15px; background:#f5f5f5; border:1px solid #e8e8e8;}
.personal_coupon h5{line-height:30px; cursor:pointer;}
.personal_coupon h5:hover{color:#c00;}
.personal_coupon_head li{width:100px; font-size:14px; color:#333; line-height:30px;}
.personal_coupon_head li input{width:15px; height:15px; margin:8px 5px 0 0; cursor:pointer;}


.no_data{height:230px; text-align:center;}
.no_data_w{padding:50px 0 30px;}
.no_data_w img{width:40px; height:40px;}
.no_data_w p{font-size:14px; color:#999; line-height:15px; margin-top:20px;}

/*详情*/
/*订单详情*/
.dingdan_detail{padding:0 10px;}
.dingdan_detail_lists{margin:15px 0 20px;}
.dingdan_detail_lists li{width:25%; }
.dingdan_detail_lists li h6{font-size:13px; font-weight:600; margin-bottom:15px; color:#666;}
.dingdan_detail_lists li dl dd{margin-top:8px; overflow: hidden;}
.dingdan_detail_lists li dl dd em{color:#999; float:left;}
.dingdan_detail_lists li dl dd span{width:170px; float:left;}
/*订单备注*/
.dingdan_detail_beizhu{margin-top:15px;}
.dingdan_detail_beizhu h6{font-size:13px; color:#666; font-weight:600; margin-bottom:15px;}
.dingdan_detail_beizhu div{border:1px solid #ddd; padding:8px 10px; font-size:13px; line-height:20px; min-height:40px;}
/*订单商品详情*/
.dingdan_detail_shops{margin-top:15px;}
.dingdan_detail_shops h6{font-size:13px; color:#666; font-weight:600; margin-bottom:15px;}
.dingdan_detail_shops table{margin:0;}
.dingdan_detail_shops table thead{border:1px solid #ddd; border-top:2px solid #ccc;}
.dingdan_detail_shops_lists li{border:1px solid #ddd; border-top:none; padding:10px 5px;}
.dingdan_detail_shops_lists li .food_name{width:40%;}
.dingdan_detail_shops_lists li .food_name img{width:90px; height:90px; border:1px solid #ddd; margin-left:10px;}
.dingdan_detail_shops_lists li .food_name p{margin-left:10px; width:220px; margin-top:25px; line-height:20px; font-size:12px; min-height:40px;}
.dingdan_detail_shops_lists li .food_pic{width:20%; text-align:center; margin-top:25px;}
.dingdan_detail_shops_lists li .food_mun{width:20%; text-align:center; margin-top:25px;}
.dingdan_detail_shops_lists li .food_state{width:20%; text-align:center; margin-top:25px;}
.dingdan_detail_shops_lists li .food_oper span{display:inline-block; width:70px; height:25px; line-height:25px; text-align: center; background:#F55567; cursor:pointer; border-radius:2px;}
.dingdan_detail_shops_lists li .food_oper em{display:inline-block; width:70px; height:25px; line-height:25px; text-align: center; background:#FF9900; cursor:pointer; border-radius:2px;}