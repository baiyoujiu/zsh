.table {
    margin-bottom: 60px;
}

.table thead>tr>th {
    background: #f7f7f7;
    line-height: 16px;
    font-size: 14px;
    color: #333;
    font-weight: normal;
}

.table-bordered,
.table-bordered>thead>tr>th,
.table-bordered>tbody>tr>th,
.table-bordered>tfoot>tr>th,
.table-bordered>thead>tr>td,
.table-bordered>tbody>tr>td,
.table-bordered>tfoot>tr>td {
    border: none;
    text-align: center;
    padding: 16px;
    font-size: 14px;
}

.table>tbody>tr {
    border-bottom: 1px #eaeaea solid;
}

.table>tbody>tr>td {
    line-height: 34px;
    color: #666
}

.table>tbody>tr>td>p {
    line-height: 20px;
}

.handle a {
    font-size: 14px;
}

.table-bordered>thead>tr>th:nth-child(1) {
    padding-left: 36px;
}

.table-bordered>thead>tr>th:nth-last-child(1) {
    padding-left: 24px;
}

.table-bordered>tbody>tr>td:nth-child(1) {
    padding-left: 36px;
}

.table>tbody>tr>td>p {
    width: 195px;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box; /** 对象作为伸缩盒子模型显示 **/
    -webkit-box-orient: vertical; /** 设置或检索伸缩盒对象的子元素的排列方式 **/
    -webkit-line-clamp: 4; /** 显示的行数 **/
    overflow: hidden;
}


