.spqhtop{width:1200px;height:185px;background:#fff;border-radius:6px;margin-top:30px;}

.cppart2{width:1200px;height:1070px;background:#fff;border-radius:6px;margin-top:20px;}
.cppart2 .cpmain{width:1120px; padding:20px 40px;}
.cppart2 .cpmain .itemlist{min-height:50px;line-height:50px;position:relative;width:100%;}
.cppart2 .cpmain .itemlist.border{border-bottom:1px solid #e6e6e6;}
.cppart2 .cpmain .itemlist .itemleft{position:absolute;left:0px;top:0px;font-size:20px;font-weight:bold;width:120px;text-align:right;}
.cppart2 .cpmain .itemlist .itemright{position:absolute;left:190px;top:0px;width:930px;}
.cppart2 .cpmain .itemlist .itemright .jytab{width:100%;}
.cppart2 .cpmain .itemlist .itemright .jytab li{float:left;margin-right:70px;font-size:18px;cursor:pointer;}
.cppart2 .cpmain .itemlist .itemright .jytab li.selected{color:#f55567;border-bottom:2px solid #f55567;font-weight:bold;}
.cppart2 .cpmain .itemlist .more{position:absolute;right:10px;top:0px;}
.cppart2 .cpmain .itemlist .more a{color:#499df3;font-size:20px;}
.cppart2 .cpmain .tabcon{display:none;}
.cppart2 .cpmain .signicon{display:inline-block;width:20px;height:20px;background:url(../img/Sighbg.png) no-repeat;}
.cppart2 .cpmain .proicon{display:inline-block;width:20px;height:20px;background:url(../img/proicon.png) no-repeat;}
.cppart2 .cpmain .sign{height:20px;line-height:20px;font-size:14px;color:#777;}
.cppart2 .cpmain .sign span{display:inline-block;float:left;margin-left:10px;}

.textinput{width:100%;height:50px;border:1px solid #e3e3e3;border-radius:4px;position:relative;}
.textinput .intini{border:0px;line-height:30px;font-size:24px;position:absolute;left:10px;top:10px;width:90%;color:red;font-size:12px;}
.textinput .putdw{color:#666;line-height:30px;font-size:14px;position:absolute;right:10px;top:10px;width:10%;text-align:center;}

.jeitemlist{width:100%;clear:both;}
.jeitemlist a{width:135px;height:100px;border:1px solid #e6e6e6;display:inline-block;margin-right:25px;margin-top:20px;}
.jeitemlist a p{line-height:20px;font-size:14px;text-align:center;}
.jeitemlist a:hover{border:1px solid red;background:url(../img/jeitembg.png) bottom right no-repeat ;}
.jeitemlist a.selected{border:1px solid red;background:url(../img/jeitembg.png) bottom right no-repeat ;}

.xzitem{width:500px;display:inline-block;float:left;}
.xzitem:nth-child(2n+1){width:390px;}
.xzitem dd{width:135px;text-align:left;display:inline-block;font-size:16px;font-weight:bold;}
.xzitem dt{display:inline-block;text-align:left;font-size:16px;color:#777;}

.zftit{font-size:24px;font-weight:bold;text-align:center;color:#f3493a;}
.xytit{font-size:16px;text-align:center;}

.mssqbtn{width:310px;height:65px;line-height:65px !important;background:#f56276;font-size:24px;display:inline-block;border-radius: 4px;color: #fff !important;text-align:center;margin-top:15px;margin-left:400px;}
.btnsm{width:100%;text-align:center;}
.btnsm a{height:16px;font-size:16px;color:red;border-right:1px solid #e6e6e6;padding-right:10px;display:inline-block;margin-top:16px;padding-left:5px;}
.btnsm a:last-child{border:0px;}

.cptab{width:1200px;height:70px;background:#f75054;border-radius:15px;}
.cptab li{width:160px;height:20px;font-size:20px;display:inline-block;text-align:center;border-right:1px solid #fff;margin-top:16px;}
.cptab li:last-child{border:0px;}
.cptab li a{color:#fff;}
.cptab li a.selected{border-bottom:1px solid #fff;}

.cppart3{width:1200px;height:545px;background:#fff;border-radius: 0px 0px 6px 6px;margin-top:-15px;}
.xTable{width:1120px;padding:40px;}
.xTable table{width:100%;}
.xTable .fb{height:40px;background:#eee;}
.xTable .fb th{font-size:16px;text-align:center;}
.xTable  td{font-size:14px;text-align:center;height:50px;line-height:50px;color:#888;border-bottom:1px dashed #e3e3e3;}
.xTable  td p{line-height:20px;}

.xTable1{width:560px;}
.xTable1 table{width:100%;}
.xTable1 .fb{height:40px;background:#eee;}
.xTable1 .fb th{font-size:16px;text-align:center;}
.xTable1  td{font-size:14px;text-align:center;height:24px;line-height:24px;color:#888;border-bottom:1px dashed #e3e3e3;}
.xTable1 tr:hover td{background:#fbfbfb;}
.cpnews{display:none;}

.cppart4{width:1200px;height:790px;background:#fff;border-radius:6px;}
.cppart4 .cptit{height:48px;width:148px;line-height:48px;text-align:center;background:url(../img/cptitbg.png) no-repeat;color:#fff;font-size:24px;font-weight:bold;}
.cppart4 .jypz{width:1120px;margin:0px 40px;padding:40px 0px;border-bottom:1px solid #e6e6e6;}
.cppart4 .jypz p{line-height:30px;font-size:16px;}
.cppart4 .jypz p .bigfont{font-weight:bold;}

.cppart4 .cpicon{width:1121px;height:160px;background:url(../img/cptubiao.png) top center no-repeat ;margin:30px auto;}
.cppart4 .cpicon li{width:90px;text-align:center;padding-top:100px;display:inline-block;margin-right:112px;}
.cppart4 .cpicon li:last-child{margin:0px;}
.cppart4 .cpicon li span{display:block;font-size:16px;line-height:22px;}

/*阴影*/
.popup_bg{display:none; width:100%;height:100%; position:absolute;left:0;top:0;z-index:99998; background:#333; opacity:0.3;}
/*发布弹窗*/
.text_null{display:none;min-width:410px; background:#fff;min-height:170px; height:150px; border-radius:6px 6px; z-index:999999999999999; position:fixed; left:54%; top:40%; margin-left:-200px;}
.text_null_title{min-width:400px; line-height:40px; border-bottom:1px solid #F56276; text-align:center; font-size:16px; font-weight:600; color:#F56276;}
.text_null_content{text-align:center; line-height:70px; font-size:20px;padding: 5px 10px;}
.text_null_btn span{width:80px; height:30px; background:#009EE8; color:#fff; text-align:center; line-height:30px; border-radius:3px 3px; margin-right:15px; cursor:pointer;}
.text_null_content .gp_dui{background:url(../images/gp-dui.png) no-repeat; width:55px; height:38px; display: block; margin:17px 0 0 70px;}
.text_null_content .gp_cuo{background:url(../images/gp-cuo.png) no-repeat; width:55px; height:38px; display: block; margin:17px 0 0 8px;}